


pcm.mute4 {
  @args.0 SLAVE
  @args.SLAVE {
    type string
    default "plughw:0,0"
  }
  type route
  slave {
    pcm $SLAVE
    channels 6
  }
  ttable {
    0.0= 1
    1.1= 1
    2.2= 1
    3.3= 1
    4.4= 0
    5.5= 1
  }
}



pcm.play4 {
  @args.0 SLAVE
  @args.SLAVE {
    type string
    default "plughw:0,0"
  }
  type route
  slave {
    pcm $SLAVE
    channels 6
  }
  ttable {
    0.0= 0
    1.1= 0
    2.2= 0
    3.3= 0
    4.4= 1
    5.5= 0
  }
}



